<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("~/Content/css/custom-theme/jquery-ui-1.10.3.custom.min.css")">
    @Styles.Render("~/Content/css")
    <!--[if gte IE 9]>
      <style type="text/css">
        .gradient {
           filter: none;
        }
      </style>
    <![endif]-->
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Scripts/jquery.carouFredSel-6.2.1-packed.js")" type="text/javascript"></script>
     <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="http://demo.dev7studios.com/caroufredsel/wp-content/plugins/caroufredsel/scripts/jquery.imagesloaded.min.js?ver=2.1.1" type="text/javascript"></script>
</head>
<body>
    <div class="header">
        <div class="max-width">
            <a class="logo" href="@Url.Action("Index","Default")"><img src="@Url.Content("~/Content/Images/logo-credbox.png")" /></a>
            <a href="javascript:void(0)" class="btn-painelcontrole"></a>
            <a href="chat"></a>
            <form class="code-search">
                <label>
                    Encontre pelo código<br />
                    <input type="text" />
                    <button></button>
                </label>
            </form>
            <ul class="menu">
                <li><a href="#">Home</a></li>
                <li><a href="#">CredBox</a></li>
                <li><a href="#">Encontre seu Imóvel</a></li>
                <li><a href="#">Acompanhe seu Processo</a></li>
                <li><a href="#">Atendimento Instantâneo</a></li>
                <li><a href="#">Fale Conosco</a></li>
            </ul>
        </div>
    </div>
    <div class="content">

    @RenderBody()
    </div>
    <div class="footer">
        <div class="max-width">
            <a href="@Url.Action("Index","Default")"><img src="@Url.Content("~/Content/images/logo-footer.png")" /></a>
            <ul class="menu">
                <li><a href="#">Home</a></li>
                <li><a href="#">CredBox</a></li>
                <li><a href="#">Encontre seu Imóvel</a></li>
                <li><a href="#">Acompanhe seu Processo</a></li>
                <li><a href="#">Atendimento Instantâneo</a></li>
                <li><a href="#">Fale Conosco</a></li>
            </ul>
            <small>Todos os Direitos Reservados</small>
        </div>
        
    </div>
    <div class="modal-interessei2"></div>
    <div class="modal-restrito">
        <div class="amount-modal">
            <div class="title-modal">
                Identifique-se
            </div>
            <div class="content-modal">
                <form id="login">
                    <label>E-mail<br /><input type="text" /></label>
                    <label>Senha<br /><input type="password" /></label>
                    <button class="float-l">Esqueci a Senha</button><button class="float-r">Acessar</button>
                </form><div class="clear"></div>
                <form id="lembrasenha" style="display:none;">
                    <p>Preencha o campo abaixo.</p>
                    <label>E-mail<br /><input type="text" /></label>
                    <button class="float-l">Voltar</button><button class="float-r">Enviar Email</button>
                </form>
            </div>
        </div>
    </div>
</body>
<script>
    jQuery(document).ready(function () {
        $(".btn-painelcontrole").click(function () {
            $(".modal-restrito, .modal-interessei2").show();
        });

        $(".modal-interessei2").click(function () {
            $(".modal-restrito, .modal-interessei2").hide();
        });
    });
</script>
</html>
