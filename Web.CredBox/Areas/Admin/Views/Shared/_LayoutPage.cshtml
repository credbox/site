<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("~/static/css/areas/admin/style.css")" />
    <link href="@Url.Content("~/static/css/areas/admin/bootstrap-responsive.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/static/css/areas/admin/bootstrap.min.css")" rel="stylesheet" />


    @Styles.Render("~/Content/css")
    <!--[if gte IE 9]>
      <style type="text/css">
        .gradient {
           filter: none;
        }
      </style>
    <![endif]-->
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>

</head>
<body>
    <div class="container">
        <div class="page-header">
            <h1>Teste</h1>
        </div>
        <div class="row">
            <div id="maincontent" class="span8">
                <div class="tabbable">
                    <ul class="nav nav-pills">
                        <li class="active"><a href="@Url.Action("List","Assunto")" data-toggle="tab">Assunto</a></li>
                        <li><a href="@Url.Action("List","Categoria")" data-toggle="tab">Categoria</a></li>
                        <li><a href="@Url.Action("List","Tipo")" data-toggle="tab">Tipo</a></li>
                        <li><a href="#jquery" data-toggle="tab">the jQuery</a></li>
                    </ul>
                    <div class="tab-content">
                        <div id="demo" class="tab-pane active">
                            <div id="contact-form" class="form-horizontal">
                                @RenderBody()
                            </div>
                        </div>

                    </div><!-- .tab-content -->
                </div><!-- .tabbable -->
            </div><!-- .span -->
        </div><!-- .row -->
        <hr>
        <footer>
            @*<p>Martelli Soluções</p>*@
        </footer>

    </div> <!-- .container -->


</body>
</html>
